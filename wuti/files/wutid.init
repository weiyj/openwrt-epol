USE_PROCD=1
NAME=wutid
PROG=/sbin/wuti

START=10
STOP=15

start_service() {
    procd_open_instance
    procd_set_param command "$PROG"
    procd_close_instance
}

stop() {
    service_stop /sbin/wuti
}

boot () {
    start
}
